{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "BIG Smart Production"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "images"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "tsconfig": "tsconfig.app.json",
      "prefix": "app",
      "styles": [
        "styles/style.css"
      ],
      "scripts": [
        "scripts/jquery.js",
        "scripts/jquery-ui.min.js",
        "scripts/device-resources.js",
        "scripts/device-updating-states.js",
        "scripts/model-arrow.js",
        "scripts/model-device.js",
        "scripts/model-diagram.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
